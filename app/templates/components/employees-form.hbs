{{yield}}

  {{#each employees as |employee|}}
    <tr>
        <td>{{employee.nome}}</td>
        <td>{{employee.email}}</td>
        <td class="actions">
            <button type="button" class="btn btn-warning btn-xs">Editar</button>
            <button type="button" class="btn btn-danger btn-xs">Excluir</button>
        </td>
    </tr>
  {{/each}}